    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "4.12.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
